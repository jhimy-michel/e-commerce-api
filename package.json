{
  "name": "ecommerce-api",
  "version": "0.0.1",
  "description": "API for e commerce App",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "lb-tsc --copy-resources",
    "build:watch": "lb-tsc --watch",
    "copy-env": "cp ./src/secrets/.env ./dist/secrets/.env",
    "lint": "npm run eslint && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "posttest": "npm run lint",
    "premigrate": "npm run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "npm run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "dev": "nodemon -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "npm run clean && npm run build && npm run copy-env",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^8.1.2",
    "@loopback/authentication-jwt": "^0.11.2",
    "@loopback/boot": "^4.1.2",
    "@loopback/core": "^3.1.2",
    "@loopback/openapi-v3": "^7.1.2",
    "@loopback/repository": "^4.1.2",
    "@loopback/rest": "^11.1.2",
    "@loopback/rest-explorer": "^4.1.2",
    "@loopback/security": "^0.7.2",
    "@loopback/service-proxy": "^4.1.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.8",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.merge": "^4.6.2",
    "loopback-connector-mongodb": "^5.6.0",
    "mongodb": "^4.5.0",
    "nanoid": "^3.3.3",
    "sleep": "^6.3.0",
    "source-map-support": "^0.5.21",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^8.1.2",
    "@loopback/eslint-config": "^12.0.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^12.20.47",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemailer-express-handlebars": "^4.0.2",
    "@types/speakeasy": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "husky": "^8.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.16",
    "prettier": "^2.0.5",
    "typescript": "~4.6.3"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  }
}
