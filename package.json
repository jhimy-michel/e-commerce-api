{
  "name": "e-commerce-api",
  "version": "1.0.0",
  "description": "e commerce api",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc --copy-resources",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run prettier:check && yarn run eslint",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier  './src/**/*.ts' -l",
    "prettier:check": "yarn run prettier:cli",
    "prettier:fix": "yarn run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint -- --fix",
    "pretest": "yarn run clean && yarn run build",
    "posttest": "yarn run lint",
    "docker:build": "docker build -t e-commerce-api .",
    "docker:run": "docker run -p 3000:3000 -d e-commerce-api",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run build",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.3.5",
    "@loopback/core": "^2.9.1",
    "@loopback/openapi-v3": "^3.4.5",
    "@loopback/repository": "^2.9.0",
    "@loopback/rest": "^5.2.0",
    "@loopback/rest-explorer": "^2.2.6",
    "@loopback/service-proxy": "^2.3.4",
    "loopback-connector-mongodb": "^6.2.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.1.0",
    "@loopback/eslint-config": "^8.0.3",
    "@loopback/testlab": "^3.2.0",
    "@types/node": "^10.17.26",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^7.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.0.5",
    "source-map-support": "^0.5.19",
    "typescript": "~3.9.5"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  }
}
