{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "@loopback/eslint-config"
    ],
    "overrides": [],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "curly": "error",
        "no-unused-vars": "warn",
        "@typescript-eslint/naming-convention": "warn",
        "@typescript-eslint/no-unused-vars": [
            "warn"
        ]
    }
}